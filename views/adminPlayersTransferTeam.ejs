<% include ./partials/header %>
<link rel="stylesheet" type="text/css" href="/css/adminPlayers.css">

<div class="container">
    <div id="players">
        <table border="1">
            <tr>
                <th>Засах</th>
                <th>Баг</th>
                <th>Зураг</th>
                <th>Нэр</th>
                <th>Бүтэн нэр</th>
            </tr>
            <% var keys = Object.keys(players); %>
            <% for (var i = 0; i < keys.length; i++) { %>
                <tr>
                    <td><a href="/admin/players/transfer/<%= players[ keys[i] ].playerTeamName %>/<%= keys[i] %>" id="edit<%= i %>">Засах</a></td>
                    <td><%= players[ keys[i] ].playerTeamName %></td>
                    <td><img src="<%= players[ keys[i] ].img %>"></td>
                    <td><%= players[ keys[i] ].playerName %></td>
                    <td><%= players[ keys[i] ].playerFullName %></td>
                </tr>
            <% } %>
        </table>
    </div>
</div>


<script src="/js/adminPlayers.js"></script>
<% include ./partials/footer %>