<% include ./partials/header %>
<link rel="stylesheet" type="text/css" href="/css/adminTeams.css">

<div class="container">
    <div id="teams">
        <table border="1">
            <tr>
                <th>Засах</th>
                <th>Зураг</th>
                <th>Нэр</th>
                <th>Бүтэн нэр</th>
                <th>Элссэн он</th>
                <th>Багийн цахим хаяг</th>
            </tr>
            <% var keys = Object.keys(teams); %>
            <% for (var i = 0; i < keys.length; i++) { %>
                <tr>
                    <td><a href="/admin/edit/team/<%= keys[i] %>" id="edit<%= i %>">Засах</a></td>
                    <td><img src="<%= teams[ keys[i] ].img %>"></td>
                    <td><%= teams[ keys[i] ].teamName %></td>
                    <td><%= teams[ keys[i] ].teamFullName %></td>
                    <td><%= teams[ keys[i] ].teamDebut %></td>
                    <td><%= teams[ keys[i] ].teamWeb %></td>
                </tr>
            <% } %>
        </table>
    </div>
</div>


<script src="/js/adminTeams.js"></script>
<% include ./partials/footer %>