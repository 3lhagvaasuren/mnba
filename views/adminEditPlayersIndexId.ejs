<% include ./partials/header %>
<link rel="stylesheet" type="text/css" href="/css/adminEditPlayersIndexId.css">

<div class="container">
    <div id="teams">
        <table border="1">
            <tr>
                <th>Засах</th>
                <th>Индекс</th>
                <th>Зураг</th>
                <th>Нэр</th>
                <th>Бүтэн нэр</th>
            </tr>
            <% var keys = Object.keys(teamPlayers);%>
            <% for (var i = 0; i < keys.length; i++) { %>
                <% var player = teamPlayers[ keys[i] ]; %>
                <tr>
                    <td><a href="#" class="change-index" id="i<%= player.playerId %>">Солих</a></td>
                    <td id="x<%= player.playerId %>"><%= player.playerIndex %></td>
                    <td><img src="<%= player.img %>"></td>
                    <td><%= player.playerName %></td>
                    <td><%= player.playerFullName %></td>
                </tr>
            <% } %>
        </table>
    </div>
</div>
<p class="hidden" id="team-name"><%= team %></p>
<p class="hidden" id="current-year"><%= currentYear %></p>
<script src="/js/adminEditPlayersIndexId.js"></script>
<% include ./partials/footer %>